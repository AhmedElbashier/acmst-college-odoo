<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- University Sequence -->
        <record id="seq_university_code" model="ir.sequence">
            <field name="name">University Code</field>
            <field name="code">acmst.university</field>
            <field name="prefix">UNI</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <!-- College Sequence -->
        <record id="seq_college_code" model="ir.sequence">
            <field name="name">College Code</field>
            <field name="code">acmst.college</field>
            <field name="prefix">COL</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <!-- Program Type Sequence -->
        <record id="seq_program_type_code" model="ir.sequence">
            <field name="name">Program Type Code</field>
            <field name="code">acmst.program.type</field>
            <field name="prefix">PT</field>
            <field name="padding">2</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <!-- Program Sequence -->
        <record id="seq_program_code" model="ir.sequence">
            <field name="name">Program Code</field>
            <field name="code">acmst.program</field>
            <field name="prefix">PROG</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <!-- Batch Sequence -->
        <record id="seq_batch_code" model="ir.sequence">
            <field name="name">Batch Code</field>
            <field name="code">acmst.batch</field>
            <field name="prefix">BATCH</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <!-- Academic Year Sequence -->
        <record id="seq_academic_year_code" model="ir.sequence">
            <field name="name">Academic Year Code</field>
            <field name="code">acmst.academic.year</field>
            <field name="prefix">AY</field>
            <field name="padding">2</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <!-- Academic Rules Sequence -->
        <record id="seq_academic_rules_code" model="ir.sequence">
            <field name="name">Academic Rules Code</field>
            <field name="code">acmst.academic.rules</field>
            <field name="prefix">RULE</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
    </data>
</odoo>
