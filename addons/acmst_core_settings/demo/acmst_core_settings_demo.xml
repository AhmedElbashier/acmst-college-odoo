<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Demo University -->
        <record id="demo_university_acmst" model="acmst.university">
            <field name="name">ACMST College of Technology</field>
            <field name="code">ACMST</field>
            <field name="short_name">ACMST</field>
            <field name="address">123 Education Street, Tech City</field>
            <field name="city">Tech City</field>
            <field name="zip">12345</field>
            <field name="phone">+1-555-0123</field>
            <field name="email">info@acmst.edu</field>
            <field name="website">https://www.acmst-edu.com</field>
            <field name="established_year">1995</field>
            <field name="is_main">True</field>
            <field name="description">A leading institution in technology and management education, committed to excellence in academic programs and student success.</field>
        </record>
        
        <!-- Demo Colleges -->
        <record id="demo_college_engineering" model="acmst.college">
            <field name="name">College of Engineering</field>
            <field name="code">ENG</field>
            <field name="short_name">Engineering</field>
            <field name="university_id" ref="demo_university_acmst"/>
            <field name="established_year">1995</field>
            <field name="program_types">both</field>
            <field name="description">Leading college in engineering education with state-of-the-art facilities and industry partnerships.</field>
            <field name="mission">To provide world-class engineering education and research opportunities.</field>
            <field name="vision">To be the premier engineering college recognized globally for innovation and excellence.</field>
        </record>
        
        <record id="demo_college_business" model="acmst.college">
            <field name="name">College of Business Administration</field>
            <field name="code">BUS</field>
            <field name="short_name">Business</field>
            <field name="university_id" ref="demo_university_acmst"/>
            <field name="established_year">1998</field>
            <field name="program_types">both</field>
            <field name="description">Comprehensive business education preparing students for leadership roles in the global economy.</field>
            <field name="mission">To develop ethical business leaders through innovative education and practical experience.</field>
            <field name="vision">To be recognized as a top business school producing successful entrepreneurs and executives.</field>
        </record>
        
        <record id="demo_college_computer_science" model="acmst.college">
            <field name="name">College of Computer Science</field>
            <field name="code">CS</field>
            <field name="short_name">Computer Science</field>
            <field name="university_id" ref="demo_university_acmst"/>
            <field name="established_year">2000</field>
            <field name="program_types">both</field>
            <field name="description">Cutting-edge computer science programs preparing students for the digital future.</field>
            <field name="mission">To advance computer science education and research in emerging technologies.</field>
            <field name="vision">To be a leading computer science college driving technological innovation.</field>
        </record>
        
