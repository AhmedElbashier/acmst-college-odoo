            <field name="duration_years">4</field>
            <field name="is_thesis_required">True</field>
            <field name="is_internship_required">True</field>
            <field name="thesis_credits">8</field>
            <field name="internship_credits">6</field>
            <field name="description">Engineering-focused program emphasizing software design, development, and project management.</field>
