            <field name="description">Comprehensive business administration program covering management, marketing, finance, and operations.</field>
            <field name="objectives">To prepare students for leadership roles in business and entrepreneurship.</field>
        </record>
        
        <record id="demo_program_cs_master" model="acmst.program">
            <field name="name">Master of Science in Computer Science</field>
            <field name="code">MSCS</field>
            <field name="short_name">MSc CS</field>
            <field name="college_id" ref="demo_college_computer_science"/>
