        <!-- Demo Programs -->
        <record id="demo_program_cs_bachelor" model="acmst.program">
            <field name="name">Bachelor of Science in Computer Science</field>
            <field name="code">BSCS</field>
            <field name="short_name">BSc CS</field>
            <field name="college_id" ref="demo_college_computer_science"/>
            <field name="program_type_id" ref="program_type_bachelor"/>
            <field name="credits_required">120</field>
            <field name="duration_years">4</field>
            <field name="is_thesis_required">True</field>
            <field name="is_internship_required">True</field>
            <field name="thesis_credits">6</field>
            <field name="internship_credits">3</field>
            <field name="description">Comprehensive computer science program covering software development, algorithms, and system design.</field>
            <field name="objectives">To prepare students for careers in software development, system analysis, and technology leadership.</field>
        </record>
        
        <record id="demo_program_engineering_bachelor" model="acmst.program">
            <field name="name">Bachelor of Engineering in Software Engineering</field>
            <field name="code">BESE</field>
            <field name="short_name">BE SE</field>
